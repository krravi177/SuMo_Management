<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
>
    <Dialog id="createSubProjectDialog" title="Create New SubProject"  >
    <VBox id="createSubProjectVBox" width="30%" class="sapUiSmallMarginBeginEnd" >
        <Label text="Module ID" />
        <Input id="npModuleId" />

        <Label text="Project ID" />
        <Input id="npSPProjectID" />

        <Label text="Assigned To" />
        <Input id="npAssignedToE" />

        <Label text="Time Assigned" />
        <Input id="npTimeSPAssigned" />

        <Label text="ProjectManager ID" />
        <Input id="npProjectManagerID" />

        <Label text="Module Status" />
        <Input id="npModuleStatus" />

        <Label text="Created By" />
        <Input id="npCreatedBy" />

        <Label text="Modified By" />
        <Input id="npModifiedBy" />

        <Label text="Status" />
        <Input id="npSPStatus" />

        <Label text="Start Date" />
        <Input id="npSPStartDate" />

        <Label text="End Date" />
        <Input id="npSPEndDate" />

        <Button
            id="newSubProjectFormSubmitBtn"
            text="Submit"
            type="Accept"
            press="onSubmitNewSubProjectForm"
        />
         <Button
            id="newSubProjectFormCancelBtn"
            text="Cancel"
            type="Reject"
            press="onCancelNewSubProjectForm"
        />
        </VBox>
    </Dialog>
</core:FragmentDefinition>
