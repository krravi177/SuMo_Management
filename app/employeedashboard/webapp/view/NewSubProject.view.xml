<mvc:View
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:f="sap.ui.layout.form"
    xmlns:uxap="sap.uxap"
   controllerName="com.dash.employeedashboard.controller.NewSubProject">
<Page id="pmSubProjectFormPage" title="Add SubProject Form" showNavButton="true" navButtonPress="onNavAdminProjectInfoPage">
        <content>
         <uxap:ObjectPageLayout id="pmSubProjectFormOP">
                <uxap:sections>
                    <uxap:ObjectPageSection
                        id="pmSubProjectFormOPSec"
                        titleUppercase="false"
                        title="General Details"
                    >
                    
                        <uxap:subSections>
                            <uxap:ObjectPageSubSection
                                id="pmSubProjectFormOPSubSec"
                                titleUppercase="false"
                            >
                            	
                                <uxap:blocks>
                                        <f:SimpleForm
            id="pmSubProjectForm"
            editable="true"
            layout="ColumnLayout"
           
            columnsL="2"
            columnsM="1"
            singleContainerFullSize="false"
        >
            <f:content>

               <Label id="pmSubProjectFormLabModId" text="Module ID" />
        <Input id="npModuleId" />

        <Label id="pmSubProjectFormLabProjId" text="Project ID" />
        <Input id="npSPProjectID" />

        <Label id="pmSubProjectFormLabAssignTo" text="Assigned To" />
        <Input id="npAssignedToE" />

       
        <Label id="pmSubProjectFormLabPMId" text="ProjectManager ID" />
        <Input id="npProjectManagerID" />

                </f:content>
                </f:SimpleForm>

                  </uxap:blocks>
                            </uxap:ObjectPageSubSection>
                        </uxap:subSections>
                    </uxap:ObjectPageSection>
                    <uxap:ObjectPageSection
                        id="pmSubProjectFormSec2"
                        titleUppercase="false"
                        title="Status"
                    >
                    
                        <uxap:subSections>
                            <uxap:ObjectPageSubSection
                                id="pmSubProjectFormSubSec2"
                                titleUppercase="false"
                            >
                            	
                                <uxap:blocks>
                                 <f:SimpleForm
            id="pmSubProjectFormSec2Form"
            editable="true"
            layout="ColumnLayout"
            columnsL="2"
            columnsM="2"
           
        >
            <f:content>
           <Label id="pmSubProjectFormLabModStatus" text="Module Status" />
                <Select id="npModuleStatus" width="100%">
                    <items>
                        <core:Item id="pmSubProjectFormLabModStatus1" text="Low" key="Low"/>
                        <core:Item id="pmSubProjectFormLabModStatus2" text="Medium" key="Medium"/>
                        <core:Item id="pmSubProjectFormLabModStatus3" text="High" key="High"/>
                    </items>
                </Select>

                    <Label id="pmSubProjectFormLabStatus" text="Status" />
                <Select id="npStatus" width="100%" selectedKey="{npStatus}">
                    <items>
                        <core:Item id="pmSubProjectFormLabStatus1" text="Running" key="OnTime"/>
                        <core:Item id="pmSubProjectFormLabStatus2" text="Delayed" key="Delayed"/>
                        <core:Item id="pmSubProjectFormLabStatus3" text="Completed" key="Completed"/>
                    </items>
                </Select>
               
                   </f:content>
                </f:SimpleForm>
                
                 
                   </uxap:blocks>
                            </uxap:ObjectPageSubSection>
                        </uxap:subSections>
                    </uxap:ObjectPageSection>
 
                  <uxap:ObjectPageSection
                        id="pmSubProjectFormSec3"
                        titleUppercase="false"
                        title="Duration"
                    >
                    
                        <uxap:subSections>
                            <uxap:ObjectPageSubSection
                                id="pmSubProjectFormSubSec3"
                                titleUppercase="false"
                            >
                            	
                                <uxap:blocks>
                                 <f:SimpleForm
            id="pmSubProjectFormForm3"
            editable="true"
            layout="ColumnLayout"
            columnsL="2"
            columnsM="2"
        >
            <f:content>
                     <Label id="pmSubProjectFormLabTimeAssign" text="Time Assigned" />
                      <Input id="npTimeSPAssigned" />

                     <Label id="pmSubProjectFormLabModStartDate" text="Start Date" />
                      <Input id="npSPStartDate" />

                        <Label id="pmSubProjectFormLabModEndDate" text="End Date" />
                       <Input id="npSPEndDate" />

                       <Label id="pmSubProjectFormCreatBy" text="Created By" />
        <Input id="npCreatedBy" />

        <Label id="pmSubProjectFormModifyBy" text="Modified By" />
        <Input id="npModifiedBy" />
                 
                </f:content>
                </f:SimpleForm>
               
                </uxap:blocks>
                            </uxap:ObjectPageSubSection>
                        </uxap:subSections>
                    </uxap:ObjectPageSection>
                
         <uxap:ObjectPageSection
                        id="pmSubProjectFormSec4"
                        titleUppercase="false"
                        title="Finish"
                    >
                    
                        <uxap:subSections>
                            <uxap:ObjectPageSubSection
                                id="pmSubProjectFormSubSec4"
                                titleUppercase="false"
                            >
                            	
                                <uxap:blocks>
                              

                       </uxap:blocks>
                            </uxap:ObjectPageSubSection>
                        </uxap:subSections>
                    </uxap:ObjectPageSection>
 
                </uxap:sections>
            </uxap:ObjectPageLayout>
</content>
<footer>
<Toolbar id="pmSubProjectFormfooterToolbar">
            <ToolbarSpacer id="pmSubProjectFormfooterToolbarSp"/>
             <Button
            id="newSubProjectFormSubmitBtn"
            text="Submit"
            type="Accept"
            press="onSubmitNewSubProjectForm"
        />
         <Button
            id="newSubProjectFormCancelBtn"
            text="Cancel"
            type="Reject"
            press="onCancelNewSubProjectForm"
        />
        </Toolbar>
</footer>
       </Page> 
</mvc:View>

