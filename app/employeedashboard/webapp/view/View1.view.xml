<mvc:View controllerName="com.dash.employeedashboard.controller.View1"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m">
    <Page id="homePage"  class="sapUiResponsiveContentPadding">
	<headerContent>
	<Image src="images/logonew.png" decorative="false" width="80px" height="35px">
				<layoutData>
					<OverflowToolbarLayoutData priority="NeverOverflow"/>
				</layoutData>
			</Image>
			<ToolbarSpacer />
			</headerContent>
		<content>
	
	<GenericTile class="sapUiMediumMarginBegin sapUiMediumMarginTop tileLayout" header="Admin"
		 press=".onNavToAdmin">
		<TileContent  footer="Admin Information">
			<ImageContent src="sap-icon://account" />
		</TileContent>
	</GenericTile>
	<GenericTile class="sapUiMediumMarginBegin sapUiMediumMarginTop tileLayout" header="Employees"
		 press=".onNavToEmployees">
		<TileContent  footer="Employee Information">
			<ImageContent src="sap-icon://account" />
		</TileContent>
	</GenericTile>
	<GenericTile class="sapUiMediumMarginBegin sapUiMediumMarginTop tileLayout" header="ProjectManager"
		 press=".onNavToProjectManager">
		<TileContent  footer="ProjectManager Information">
			<ImageContent src="sap-icon://account" />
		</TileContent>
	</GenericTile>
	<GenericTile class="sapUiMediumMarginBegin sapUiMediumMarginTop tileLayout" header="List of Holidays"
		 press=" onPressHolidayListTile">
		<TileContent  footer="Everyone">
			<ImageContent src="sap-icon://calendar" />
		</TileContent>
	</GenericTile>
	<GenericTile class="sapUiMediumMarginBegin sapUiMediumMarginTop tileLayout" header="Not found page"
		 press="openAkibBhaiProject">
		<TileContent  footer="FilterBar">
			<ImageContent src="sap-icon://home-share" />
		</TileContent>
	</GenericTile>
	<GenericTile class="sapUiMediumMarginBegin sapUiMediumMarginTop tileLayout" header="Bank Account Information"
		 press="">
		<TileContent  footer="Admin Information">
			<ImageContent src="sap-icon://account" />
		</TileContent>
	</GenericTile>
	<GenericTile class="sapUiMediumMarginBegin sapUiMediumMarginTop tileLayout" header="Apply Leave"
		 press="">
		<TileContent  footer="Admin Information">
			<ImageContent src="sap-icon://account" />
		</TileContent>
	</GenericTile>
	<GenericTile class="sapUiMediumMarginBegin sapUiMediumMarginTop tileLayout" header="My Inbox"
		 press="">
		<TileContent  footer="Admin Information">
			<ImageContent src="sap-icon://account" />
		</TileContent>
	</GenericTile>
		</content>
    </Page>
</mvc:View>